{
  "version": 2,
  "functions": {
    "api/upload.js": {
      "runtime": "nodejs18.x"
    },
    "api/imgbb.js": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    { "src": "/api/upload", "dest": "/api/upload.js" },
    { "src": "/api/imgbb", "dest": "/api/imgbb.js" },
    { "src": "/(.*)", "dest": "/$1" }
  ]
}
