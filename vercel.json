{
  "version": 2,
  "functions": {
    "api/upload.js": { "runtime": "nodejs16.x" },
    "api/imgbb.js": { "runtime": "nodejs16.x" }
  },
  "routes": [
    { "src": "/api/upload", "dest": "/api/upload.js" },
    { "src": "/api/imgbb", "dest": "/api/imgbb.js" },
    { "src": "/(.*)", "dest": "/$1" }
  ]
}
